{
  "name": "react-pwa",
  "version": "0.2.0",
  "description": "A lite seed to develop react packages.",
  "scripts": {
    "precommit": "pretty-quick --staged",
    "dev": "NODE_ENV=development webpack-serve",
    "clean": "rm -rf dist",
    "build": "NODE_ENV=production webpack",
    "pwa": "workbox injectManifest workbox.config.js ",
    "afterclean": "rm static.tar.gz",
    "tarball": "tar -zcvf static.tar.gz -C dist/client/ .",
    "deploy": "ansible babycherry -m unarchive -a \"src=static.tar.gz dest=/usr/share/nginx/pwa.babycherry.cn\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
